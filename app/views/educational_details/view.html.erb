<h1>Educational Details</h1><br>
<table >
  <tr>
    <th>S.no</th>
    <th>Employee ID</th>
    <th>Name</th>
    <th>Education level</th>
    <th>Stream</th>
    <th>Percentage</th>
    <th>Year of completion</th>
    <th>Institution</th>
    <th>Location</th>
  </tr>
  <% EducationalDetail.all.each do |person|  %>
    <% if(person.comp_id=='1') %>
      <tr>
        <td><%= person.id %></td>
        <td><%= person.emp_id %></td>
        <% temp=PersonalDetail.find_by_comp_id_and_emp_id( '1', person.emp_id) %>
        <td><%= temp.name %></td>
        <td><%= person.education_type %></td>
        <td><%= person.stream %></td>
        <td><%= person.percentage %></td>
        <td><%= person.year_completion %></td>
        <td><%= person.institution %></td>
        <td><%= person.location %></td>

    <%end %>

    </tr>
  <% end %>

</table>
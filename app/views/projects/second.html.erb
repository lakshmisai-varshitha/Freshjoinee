<h1>Fill the required details</h1>

<%= form_tag("/projects/forSaving", method: "get") do %>
  <%= label_tag(:name, "name:") %>
  <%= text_field_tag(:name) %><br/>
  <%= label_tag(:email, "email:") %>
  <%= text_field_tag(:email) %><br/>
  <%= label_tag(:password, "password:") %>
  <%= text_field_tag(:password) %><br/>



  <%= label_tag(:department, "department:") %>
  <%= text_field_tag(:department) %><br/>

  <%=label_tag(:personal_details,"personal_details") %>
  <%= check_box_tag (:personal_details) %><br/>
  <%=label_tag(:finance_details,"finance_details") %>
  <%= check_box_tag (:finance_details) %>











  <%= submit_tag("Submit") %>
<% end %>






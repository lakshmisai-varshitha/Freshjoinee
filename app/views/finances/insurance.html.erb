<h1>Insurance Details</h1>
<% if (@in).nil? %>

<%else %>
  <table>
    <tr>
      <th>Id</th>
      <th>Relation Type</th>
      <th>Name</th>
      <th>Date Of Birth</th>
      <th>Gender</th>
    </tr>
    <% Array(@in).each do |ins| %>
      <tr>
        <td><%=link_to ins.id,finances_editinsurance_path(ins.id)%></td>
        <td><%=ins.relation_type %></td>
        <td><%=ins.name %></td>
        <td><%=ins.dob %></td>
        <td><%=ins.gender %></td>
      </tr>
      <%end %>
  </table>
<%end %>





